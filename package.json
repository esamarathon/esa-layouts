{
  "name": "esa-layouts",
  "version": "1.0.0",
  "description": "The on-screen graphics used during European Speedrunner Assembly's \"marathon\" events.",
  "homepage": "https://github.com/esamarathon/esa-layouts#readme",
  "bugs": {
    "url": "https://github.com/esamarathon/esa-layouts/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/esamarathon/esa-layouts.git"
  },
  "license": "MIT",
  "author": "zoton2",
  "contributors": [
    "BtbN"
  ],
  "scripts": {
    "autofix": "run-s autofix:*",
    "autofix:browser": "eslint --fix --ext .ts,.vue src/dashboard && eslint --fix --ext .ts,.vue src/graphics && eslint --fix --ext .ts,.vue src/browser_shared",
    "autofix:extension": "eslint --fix --ext .ts src/extension && eslint --fix --ext .d.ts src/types",
    "build": "run-s build:*",
    "build:browser": "cross-env NODE_ENV=production webpack",
    "build:extension": "tsc -b tsconfig.extension.json",
    "clean": "trash node_modules/.cache && trash dashboard && trash graphics && trash extension",
    "schema-types": "nodecg schema-types",
    "start": "node ../..",
    "watch": "run-p watch:*",
    "watch:browser": "webpack -w",
    "watch:extension": "tsc -b tsconfig.extension.json -w",
    "postinstall": "cd shared && node postinstall.js"
  },
  "dependencies": {
    "clone": "^2.1.2",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "needle": "^3.0.0",
    "sharp": "^0.29.3",
    "speedcontrol-util": "^0.6.0-RC6",
    "streamdeck-util": "^0.4.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@esamarathon/mq-events": "^1.0.1",
    "@fontsource/barlow-condensed": "^4.5.0",
    "@fontsource/montserrat": "^4.5.1",
    "@fontsource/roboto": "^4.5.1",
    "@mdi/font": "^6.5.95",
    "@types/async": "^3.2.10",
    "@types/clone": "^2.1.1",
    "@types/lodash": "^4.14.178",
    "@types/module-alias": "^2.0.1",
    "@types/needle": "^2.5.2",
    "@types/node": "^16.11.17",
    "@types/sharp": "^0.29.4",
    "@types/uuid": "^8.3.3",
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "@vue/component-compiler-utils": "^3.3.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "dayjs": "^1.10.7",
    "deepmerge": "^4.2.2",
    "eslint": "^8.5.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-vue": "^8.2.0",
    "file-loader": "^6.2.0",
    "fitty": "^2.3.5",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
    "globby": "^12.0.2",
    "gsap": "^3.9.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.4.5",
    "nodecg-cli": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "sass": "~1.32",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "trash-cli": "^5.0.0",
    "ts-loader": "^9.2.6",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.5.4",
    "vue": "^2.6.14",
    "vue-class-component": "^7.2.6",
    "vue-eslint-parser": "^8.0.1",
    "vue-hot-reload-api": "^2.3.4",
    "vue-loader": "^15.9.8",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.5.3",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.14",
    "vuedraggable": "^2.24.3",
    "vuetify": "^2.6.1",
    "vuetify-loader": "^1.7.3",
    "vuex": "^3.6.2",
    "vuex-class": "^0.3.2",
    "vuex-class-state2way": "^1.0.1",
    "vuex-module-decorators": "^1.2.0",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "nodecg": {
    "compatibleRange": "^1.8.1",
    "bundleDependencies": {
      "nodecg-speedcontrol": "^2.4.0"
    },
    "dashboardPanels": [
      {
        "name": "game-layout-override",
        "title": "Game Layout Override",
        "width": 2,
        "file": "game-layout-override.html",
        "workspace": "ESA",
