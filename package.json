    "graphics": [
      {
        "file": "transition.html",
        "width": 1920,
        "height": 1000
      },
      {
        "file": "omnibar.html",
        "width": 1920,
        "height": 80,
        "singleInstance": true
      },
      {
        "file": "countdown.html",
        "width": 1920,
