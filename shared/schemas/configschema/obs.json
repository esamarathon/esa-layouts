{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "enabled": {
      "type": "boolean",
      "default": true
    },
    "address": {
      "type": "string",
      "default": "localhost:4444"
    },
    "password": {
      "type": "string",
      "default": ""
    },
    "names": {
      "type": "object",
      "properties": {
        "sources": {
          "type": "object",
          "properties": {
            "videoPlayer": {
              "type": "string",
              "default": "Video Player Source"
            }
          }
        }
      }
    }
  },
  "required": [
    "enabled",
    "address",
    "password",
    "names"
  ]
}