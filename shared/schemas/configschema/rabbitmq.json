{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "enabled": {
      "type": "boolean",
      "default": true
    },
    "protocol": {
      "type": "string",
      "default": "amqps"
    },
    "hostname": {
      "type": "string",
      "default": "mq.esamarathon.com"
    },
    "username": {
      "type": "string",
      "default": "USERNAME"
    },
    "password": {
      "type": "string",
      "default": "PASSWORD"
    },
    "vhost": {
      "type": "string",
      "default": "esa_prod"
    }
  },
  "required": [
    "enabled",
    "protocol",
    "hostname",
    "username",
    "password",
    "vhost"
  ]
}